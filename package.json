{
  "name": "OsciumWiFiMonitor",
  "version": "0.0.1",
  "gypfile": true,
  "main": "main.js",
  "dependencies": {
    "bindings": "^1.5.0",
    "debug": "^4.3.4",
    "electron-squirrel-startup": "^1.0.0",
    "fast-glob": "^3.2.12",
    "handlebars": "^4.7.7",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "koa-router": "^12.0.0",
    "koa-websocket": "^7.0.0",
    "nan": "^2.16.0",
    "node-addon-api": "^5.0.0"
  },
  "scripts": {
    "start": "electron-forge start",
    "build": "node-gyp --debug --thin=yes rebuild",
